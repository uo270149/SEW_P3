    <rutaACaballo nombre="Barro-Celorio-Poo">
        <tipo>Senda costera</tipo>
        <dificultad>Fácil</dificultad>
        <horaInicio>11:00:00</horaInicio>
        <duracion>2 horas</duracion>
        <agencia>Aventuras en Asturias</agencia>
        <descripcion>Ruta por la senda costera desde Barro, pasando por las playas de San Martín, Portillo y Poo.</descripcion>
        <personasAdecuadas>A partir de 7 años.</personasAdecuadas>
        <lugarInicio>Barro</lugarInicio>
        <direccionInicio>Playa de Barro</direccionInicio>
        <coordenadas>
            <latitud>43.44°N</latitud>
            <longitud>4.82°W</longitud>
            <altitud>1 metros</altitud>
        </coordenadas>
        <referencias>
            <referencia valor="https://earth.google.com/web/search/playa+de+barro/@43.4352044,-4.8245876,9.44099616a,742.02336779d,35y,0h,45t,0r/data=CngaThJICiQweGQ0OWU5NmVjYmI3MzE3YjoweGI4NGM2YzZkM2U3OTUxZjQZo4kcx7S3RUAhn78asWBME8AqDnBsYXlhIGRlIGJhcnJvGAEgASImCiQJmq3tflujNEARma3tflujNMAZxSa77OUVQEAhlxgsYt-LUcAoAg">Google Earth</referencia>
